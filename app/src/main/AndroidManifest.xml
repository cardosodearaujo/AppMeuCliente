<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.newoutsourcing.walletofclients">
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_wallet"
        android:label="@string/str_app_name"
        android:roundIcon="@mipmap/ic_wallet"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".Views.Activitys.ListClientActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Views.Activitys.RegisterClientActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

        <activity
            android:name=".Views.Activitys.ErrorActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>
</manifest>